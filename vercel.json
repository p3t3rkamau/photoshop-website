{
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/index.py" }
  ],
  "env": {
    "UPLOAD_FOLDER": "static/uploads/"
  },
  "files": ["static/uploads/**/*"]
}
